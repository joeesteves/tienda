angular.module 'Tienda' 
.config ($routeProvider) ->
	$routeProvider
	.when '/productos',
		templateUrl: '<%= asset_path("productos") %>'
		controller: 'ProductosIndexController'
	.when '/productos/new',
		templateUrl: '<%= asset_path("productos/form") %>'
		controller: 'ProductosCreateController'
	.when '/productos/precios',
		templateUrl: '<%= asset_path("productos/precios") %>'
		controller: 'ProductosPreciosController'
	.when '/productos/:id',
		templateUrl: '<%= asset_path("productos/form") %>'
		controller: 'ProductosUpdateController'